#!/bin/python

print (Here is some information about planets:\n)
print (" 1).Venus 2)Mars 3)Jupiter ")
print (" 4)Saturn 5)Uranus 6)Neptune\n")


weight = 185
	planet=int(input("Enter the number for planet:))

if planet==1
	weight *=0.91
	print ("On Venus  your weigth woulde be" weight)
else
if planet ==2 
	weight *=0.38
	print ("On Mars your weight woulde be" weight)
else
if planet==3
	weight *=2.34
	print ("On Jupiter your weight woulde be" weight)

else
if planet==4
	weigth *=1.06
	print ("On Saturn  your weight woulde be" weight)
else
if planet==5
	weight *=0.92
	print ("On Uranus your weight woulde" weight)
else 
if planet==6
	weight *=1.19
	print ("On Neptune your weight woulde be" weight)
else
	print ("That planet is not listed.Please enter number 1 to 6")

